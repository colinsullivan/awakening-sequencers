{
  "name": "awakening-sequencers",
  "version": "0.8.7",
  "description": "Components for building generative music systems involving sequenced events.",
  "main": "lib/index.js",
  "scripts": {
    "start_example": "babel-node src/example.js",
    "start_parameter_example": "babel-node src/parameter_example.js",
    "start_sampler_example": "babel-node src/example_sampler.js",
    "build": "rm -Rf lib/* && babel src --out-dir lib",
    "test": "nyc mocha --timeout 10000 --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/colinsullivan/awakening-sequencers.git"
  },
  "keywords": [
    "node.js",
    "supercollider",
    "supercollider-redux"
  ],
  "author": "Colin Sullivan <colin [at] colin-sullivan.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/colinsullivan/awakening-sequencers/issues"
  },
  "homepage": "https://github.com/colinsullivan/awakening-sequencers#readme",
  "dependencies": {
    "lodash": "^4.17.10",
    "midi": "^0.9.5",
    "redux": "^3.6.0",
    "reselect": "^3.0.1",
    "supercollider-redux": "0.0.94"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true
      }
    },
    "env": {
      "browser": false,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "no-use-before-define": "error",
      "block-scoped-var": "error"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "chai": "^4.1.1",
    "mocha": "^3.5.0",
    "nyc": "^11.8.0",
    "redux-logger": "^3.0.6"
  }
}
